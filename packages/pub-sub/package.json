{
  "name": "pub-sub",
  "version": "0.1.0",
  "description": "publish-subscribe pattern",
  "keywords": [
    "publish-subscribe",
    "publisher",
    "subscriber"
  ],
  "author": "lbwa <mr_lbw@outlook.com>",
  "homepage": "https://github.com/lbwa/observer-vs-pub-sub/tree/master/packages/pub-sub#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbwa/observer-vs-pub-sub.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "npm run clear && tsc",
    "clear": "rm -rf lib/*"
  },
  "bugs": {
    "url": "https://github.com/lbwa/observer-vs-pub-sub/issues"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "preset": "ts-jest",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "\\\\examples\\\\",
      "\\\\dist\\\\",
      "\\\\lib\\\\"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    }
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  }
}